{
  "name": "first-watch-weather",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run tailwind:css\"",
    "build": "next build",
    "start": "next start",
    "tailwind:css": "tailwindcss -i ./src/app/styles/globals.css -o ./src/app/styles/output.css --watch",
    "postinstall": "husky",
    "test-pre-push": "jest --watchAll=false",
    "test-and-build": "yarn test-pre-push && yarn build",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@googlemaps/extended-component-library": "^0.6.11",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.59.15",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^11.11.1",
    "next": "^14.2.15",
    "notistack": "^3.0.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^20",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}
